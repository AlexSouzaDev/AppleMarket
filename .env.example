# Copy this to .env.local and fill in the actual values from your Supabase project
# Pooler (transaction) connection — use for runtime queries via Prisma Client
DATABASE_URL="postgresql://<USER>:<PASSWORD>@aws-<region>.pooler.supabase.com:5432/postgres?pgbouncer=true&connection_limit=1&sslmode=require"

# Direct Postgres connection — required for Prisma migrations/db push
DIRECT_URL="postgresql://<USER>:<PASSWORD>@db.<ref>.supabase.co:5432/postgres?sslmode=require"
